conn <%= name %>
  type=tunnel
  left=<%= client_ip %>
  leftsubnet=<%= client_subnet %>
  leftid=<%= client_ip %>
  right=<%= server_ip %>
  rightsubnet=<%= server_subnet %>
  rightid=<%= server_ip %>
  pfs=yes
  auto=start
  auth=esp
  phase2alg=aes256-sha1
  ike=aes256-sha1-modp1024
  authby=secret
  keyexchange=ike
  ikelifetime=28800s
  keylife=3600s
  forceencaps=yes
